{
  "name": "vite-vue3-h5",
  "type": "module",
  "version": "3.0.0",
  "private": true,
  "packageManager": "pnpm@10.29.2",
  "author": "lincenying <lincenying@qq.com>",
  "scripts": {
    "prepare[-del]": "npx simple-git-hooks",
    "del": "sh delete-ts.sh",
    "serve": "cross-env DEBUG=vite:transform vite",
    "build:test": "vite build --mode test",
    "build:staging": "vite build --mode staging",
    "build": "vite build",
    "start": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:ts": "vue-tsc -b --noEmit",
    "lint:css": "stylelint '**/*.{css,scss}' --fix",
    "lint-staged": "lint-staged",
    "copy": "deploy_dist vite-vue3-h5/dist"
  },
  "dependencies": {
    "@lincy/utils": "catalog:prod",
    "@unhead/vue": "catalog:prod",
    "@vueuse/core": "catalog:prod",
    "axios": "catalog:prod",
    "default-passive-events": "catalog:prod",
    "lodash": "catalog:prod",
    "md5": "catalog:prod",
    "pinia": "catalog:prod",
    "qs": "catalog:prod",
    "store2": "catalog:prod",
    "unhead": "catalog:prod",
    "vant": "catalog:prod",
    "vue": "catalog:prod",
    "vue-cropper": "catalog:prod",
    "vue-router": "catalog:prod"
  },
  "devDependencies": {
    "@iconify-json/mdi": "catalog:dev",
    "@lincy/eslint-config": "catalog:lint",
    "@lincy/stylelint-config": "catalog:lint",
    "@lincy/unocss-base-config": "catalog:dev",
    "@types/lodash": "catalog:types",
    "@types/node": "catalog:types",
    "@types/qs": "catalog:types",
    "@unocss/eslint-plugin": "catalog:lint",
    "@vitejs/plugin-vue": "catalog:dev",
    "@vitejs/plugin-vue-jsx": "catalog:dev",
    "@vue-macros/volar": "catalog:dev",
    "cross-env": "catalog:dev",
    "eslint": "catalog:lint",
    "lint-staged": "catalog:dev",
    "mockjs": "catalog:dev",
    "postcss": "catalog:dev",
    "postcss-px-to-viewport-8-plugin": "catalog:dev",
    "sass-embedded": "catalog:dev",
    "stylelint": "catalog:lint",
    "tslib": "catalog:dev",
    "typescript": "catalog:dev",
    "unocss": "catalog:dev",
    "unplugin-auto-import": "catalog:dev",
    "unplugin-icons": "catalog:dev",
    "unplugin-vue-components": "catalog:dev",
    "url-search-params-polyfill": "catalog:dev",
    "vite": "catalog:dev",
    "vite-plugin-inspect": "catalog:dev",
    "vite-plugin-mock": "catalog:dev",
    "vite-plugin-progress": "catalog:dev",
    "vue-macros": "catalog:dev",
    "vue-tsc": "catalog:dev"
  },
  "simple-git-hooks[-del]": {
    "pre-commit": "npm run lint-staged"
  },
  "lint-staged[-del]": {
    "*.{[jt]s?(x),vue,md}": [
      "eslint --fix"
    ],
    "*.{css,scss}": "stylelint --fix --allow-empty-input"
  }
}
